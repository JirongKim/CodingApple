ion order(){\n    var newTitle = [...title];\n    newTitle.sort(function(a, b) { // 한글 오름차순\n      return a < b ? -1 : a > b ? 1 : 0;\n    });\n    changeTitle(newTitle);\n  }\n\n  function add(t){\n    var newTitle = [...title];\n    newTitle.push(t);\n    changeTitle(newTitle);\n\n    var newLike = [...like];\n    newLike.push(0);\n    upLike(newLike);\n\n    var newDate = [...postDate];\n    newDate.push(setDate());\n    changePostDate(newDate);\n  }\n\n  function remove(idx){\n    console.log(idx);\n    var newTitle = [...title];\n    newTitle.splice(idx, 1);\n    changeTitle(newTitle);\n\n    var newLike = [...like];\n    newLike.splice(idx, 1);\n    upLike(newLike);\n\n    setModal(0);\n  }\n\n  return (\n    <div className=\"App\">\n      <div className=\"blog-title\">\n        {blogName}\n      </div>\n      <div style={ {color : 'black', fontSize : '30px'} }>\n        게시글\n      </div>\n      <button onClick={ () => { order(); } }>이름순 정렬</button>\n      {\n        title.map((t, i)=>{\n          return(\n            <div className=\"list\" key={i}>\n              <h4 onClick={() => { modal == i + 1 ? setModal(0) : setModal(i + 1); }}>{title[i]} <span onClick={ (e) => { upCntLike(i); e.stopPropagation(); } }>👍</span> {like[i]} </h4>\n              <p>{postDate[i][0]}월 {postDate[i][1]}일 {postDate[i][2]}시 {postDate[i][3]}분 발행</p>\n            </div>\n          )\n        })\n      }\n      {\n        modal ? <Modal color=\"yellow\" title={title[modal - 1]} idx={modal - 1} remove={remove}/> : null\n      }\n      \n      <input onChange={(e)=>{ setAddTitle(e.target.value); }}></input>\n      <button onClick={()=>{ if(addTitle != \"\") { add(addTitle); } }}>글 발행</button>\n\n    </div>\n  );\n}\n\nfunction Modal(props){\n  return(\n    <div className='modal' style={{background : props.color}}>\n      <h4>{props.title}</h4>\n      <p>날짜</p>\n      <p>내용</p>\n      <button onClick={()=>{props.remove(props.idx)}}>삭제</button>\n    </div>\n  );\n}\n\nexport default App;\n",{"ruleId":"21","severity":1,"message":"22","line":1,"column":8,"nodeType":"23","messageId":"24","endLine":1,"endColumn":12},{"ruleId":"25","severity":1,"message":"26","line":77,"column":42,"nodeType":"27","messageId":"28","endLine":77,"endColumn":44},{"ruleId":"25","severity":1,"message":"29","line":88,"column":42,"nodeType":"27","messageId":"28","endLine":88,"endColumn":44},"no-unused-vars","'logo' is defined but never used.","Identifier","unusedVar","eqeqeq","Expected '===' and instead saw '=='.","BinaryExpression","unexpected","Expected '!==' and instead saw '!='."]