ick={()=>{\n            dispatch(addItem([id, props.shoes[id].title]));\n          }}>주문하기</button>\n        </div>\n      </div>\n\n      <div className=\"tab-contaniner\">\n        {\n          tabs.map((element, i)=>{\n            return <div id={`tab-${i}`} className=\"tab-btn\" onClick={(e)=>{setSelectedTab(i);}}>{element}</div>\n          })\n        }\n      </div>\n\n      <div className=\"tab-content\">\n        {\n          tabContents[selectedTab]\n        }\n      </div>\n      \n    </div>\n  );\n}\n","/Users/jrkim/Desktop/Programming/WEB/CodingApple/src/routes/Cart.js",["77","78"],[],"/Users/jrkim/Desktop/Programming/WEB/CodingApple/src/store.js",["79"],[],"/Users/jrkim/Desktop/Programming/WEB/CodingApple/src/store/userSlice.js",["80"],[],"/Users/jrkim/Desktop/Programming/WEB/CodingApple/src/store/cartSlice.js",["81","82","83","84"],[],{"ruleId":"85","severity":1,"message":"86","line":1,"column":8,"nodeType":"87","messageId":"88","endLine":1,"endColumn":12},{"ruleId":"85","severity":1,"message":"89","line":3,"column":10,"nodeType":"87","messageId":"88","endLine":3,"endColumn":23},{"ruleId":"85","severity":1,"message":"90","line":4,"column":10,"nodeType":"87","messageId":"88","endLine":4,"endColumn":16},{"ruleId":"85","severity":1,"message":"91","line":4,"column":26,"nodeType":"87","messageId":"88","endLine":4,"endColumn":35},{"ruleId":"85","severity":1,"message":"92","line":4,"column":42,"nodeType":"87","messageId":"88","endLine":4,"endColumn":45},{"ruleId":"85","severity":1,"message":"93","line":4,"column":47,"nodeType":"87","messageId":"88","endLine":4,"endColumn":50},{"ruleId":"85","severity":1,"message":"94","line":10,"column":8,"nodeType":"87","messageId":"88","endLine":10,"endColumn":13},{"ruleId":"85","severity":1,"message":"95","line":19,"column":8,"nodeType":"87","messageId":"88","endLine":19,"endColumn":15},{"ruleId":"85","severity":1,"message":"96","line":20,"column":7,"nodeType":"87","messageId":"88","endLine":20,"endColumn":15},{"ruleId":"